def factorizar(n):
    if n < 0:
        return "No se puede factorizar números negativos"
    elif n == 0:
        return 1
    else: 
        resultado = 1
        while n > 0:
            resultado = resultado * n
            n = n-1
        return resultado
    
n = int(input('ingrese el número para hallar su factorial -> '))
prueba = factorizar(n)
print(prueba)
